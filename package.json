{
	"name": "@diegofrayo/bets",
	"version": "1.0.0",
	"private": true,
	"author": "@diegofrayo",
	"license": "ISC",
	"scripts": {
		"===== LINTING & FORMATTING =====": "",
		"lint": "eslint --ignore-path .gitignore --ext .ts --fix ./src/scripts/predictions",
		"lint:no-warnings": "cross-env NO_LINT_WARNINGS=true && npm run lint",
		"format": "prettier --write \"./src/**/*.ts\"",
		"format:minify": "minify-all-js src/scripts/predictions/data/raw -j",
		"===== MAIN =====": "",
		"script:browser-extension": "ts-node src/scripts/browser-extension/index.ts",
		"script:browser-extension:bundler": "node ./src/scripts/browser-extension/config/esbuild.mjs",
		"script:browser-extension:bundler:watch": "node --watch ./src/scripts/browser-extension/config/esbuild.mjs",
		"script:predictions": "ts-node src/scripts/predictions/index.ts",
		"===== NOTES =====": "",
		"avoid-type-checking": "ts-node src/scripts/bets/index.ts --transpileOnly"
	},
	"dependencies": {
		"axios": "^1.6.8",
		"dayjs": "^1.11.11",
		"js-convert-case": "^4.2.0",
		"jsdom": "^24.0.0",
		"prettier": "^3.2.5"
	},
	"devDependencies": {
		"@types/jsdom": "^21.1.6",
		"@types/node": "^20.5.6",
		"@typescript-eslint/eslint-plugin": "^7.16.1",
		"@typescript-eslint/parser": "^7.16.1",
		"esbuild": "^0.20.1",
		"esbuild-plugin-tsc": "^0.4.0",
		"eslint": "^8.57.0",
		"eslint-config-airbnb": "^19.0.4",
		"eslint-config-airbnb-base": "^15.0.0",
		"eslint-config-airbnb-typescript": "^18.0.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-import": "^2.29.1",
		"eslint-plugin-no-loops": "^0.4.0",
		"eslint-plugin-prettier": "^5.2.1",
		"eslint-plugin-unicorn": "^54.0.0",
		"minify-all-js": "^0.1.9",
		"ts-node": "^10.9.1",
		"typescript": "^5.2.2"
	}
}
